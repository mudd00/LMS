# Copy this file to `.env` and fill in your keys.
# Do NOT commit your real `.env` to source control.

# Backend API URL (change if backend runs elsewhere)
REACT_APP_API_URL=http://localhost:8080

# Socket/Realtime server URL
REACT_APP_SOCKET_URL=http://localhost:8080

# Mapbox token (put your token into .env; leave blank here)
REACT_APP_MAPBOX_TOKEN=

# 토스페이먼츠 클라이언트 키 (테스트: test_ck_, 실제: live_ck_)
REACT_APP_TOSS_CLIENT_KEY=

# Other optional settings
REACT_APP_FEATURE_FLAGS=
